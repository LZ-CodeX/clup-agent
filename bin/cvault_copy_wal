exec_path=`dirname $0`
exec_file=${exec_path}/../lib/cvault_copy_wal.py

if [ -f "${exec_file}" ];then
    /opt/csu_pyenv/bin/python ${exec_file} $*
else
    /opt/csu_pyenv/bin/python ${exec_file}c $*
fi
